<%- include("../_header") -%>

<div class="container">
  <h2>New project</h2>
  <form action="projects/create" method="POST">
    <div class="form-group">
      <label for="project-name">Project Name</label>
      <input id="project-name" type="text" name="name" placeholder="My Project">
    </div>
    <div class="form-group">
      <label for="project-url">Spreadsheet URI</label>
      <input id="project-url" type="text" name="url" placeholder="https://docs.google...">
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
  </form>
</div>

<%- include("../_footer") -%>
    